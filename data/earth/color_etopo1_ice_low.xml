<?xml version="1.0" encoding="UTF-8"?>
<!-- 
This sample dataset holds the complete information needed to set-up a visualization for the BO's ENE project. 
Author: Benno Schmidt, Department of Geodesy, Bochum University of Applied Sciences.
-->
<Dataset version="0.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="xsd/Dataset.xsd">
	<GeneralInformation>
		<Theme>
			<Title language="EN">Global Relief Model of the earth</Title>
			<Title language="DE">Globals Relief-Modell der Erde</Title>
			<Description>
			This dataset illustrates the global relief model of the earth that integrates topography and bathymetry.
			</Description>
		</Theme>
		<Unit />
		<PublicReference>NOAA NGDC 2012</PublicReference>
		<MetadataReference>color_etopo1_ice_low_meta.xml</MetadataReference>
	</GeneralInformation>
	<TableStructure>
		<Property dataType="">
			<Title>Spatial extent</Title>
			<Description />
			<GeoReference featureType="ENE:Bitmap" externalGeometries="false">
				<CRS>EPSG:4326</CRS>
			</GeoReference>
		</Property>
		<Property dataType="String">
			<Title>File name</Title>
		</Property>
		<Property dataType="String">
			<Title>Image size</Title>
			<Description>Image size in pixels</Description>
		</Property>
	</TableStructure>
	<Entries>
		<Entry>
			<Value>-180,-90,180,90</Value>
			<Value>color_etopo1_ice_low.jpg</Value>
			<Value>5400x2700</Value>
		</Entry>
	</Entries>
</Dataset>
